--utility functions

function initfunctions()


	--returns n with a relative
	--subtraction of m.
	function rsubt(n,m)
		if n<0 then l=n+m
			if l>0 then l=0 end
			return l
		else l=n-m
			if l<0 then l=0 end
			return l
		end
	end
	
	
	--returns n capped between m
	--and l, and a second output
	--indicating if it was changed.
	function cap(n,m,l)
		if     n>l  then return l,1
		elseif n<m  then return m,-1
		else             return n,0 end
	end
	
	
	function sgn0(n)
		if     n>0 then return 1
		elseif n<0 then return -1
		else            return 0 end
	end
	
	
	function ‚úΩflags(x,y,id)
		if y<0 then y=0 end
		
		--debug code
		if debug then
			debugpoint(x,y)
			debugbox(‚úΩ(x),‚úΩ(y))
		end
		
		
 	return fget(mget(‚úΩ(x),‚úΩ(y)),id)
 end
	
	function ‚úΩ(x)
		return flr(x/8)
	end
	
	function round(x)
		if x>=0 then
			return flr(x)
		else
			return ceil(x)
		end
	end
	
end
